          #{piContentAttr.id,jdbcType=VARCHAR},

      
          #{piContentAttr.contentId,jdbcType=VARCHAR},

      
          #{piContentAttr.attrName,jdbcType=VARCHAR},

      
          #{piContentAttr.attrValue,jdbcType=VARCHAR},


