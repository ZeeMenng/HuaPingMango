</update>
  
<update id="updateList">
    update pi_content_attr
    <set >
                  <if test="t.contentId != null  and t.contentId != '' " >
                     content_id = #{t.contentId,jdbcType=VARCHAR},
                  </if>
                  <if test="t.attrName != null  and t.attrName != '' " >
